#include<stdio.h>
#include<stdlib.h>
#include<unistd.h>
#include"mul-exp.c"

int main()
{
    unsigned int mul1, mul2;
    printf("input unsigned integer\n");
    scanf("%d %d", &mul1, &mul2);
    printf("result is %d", mul16(mul1, mul2));
    exit(EXIT_SUCCESS);
}

